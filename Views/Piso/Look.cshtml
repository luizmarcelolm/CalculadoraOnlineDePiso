@model Piso


<div class="text-center">

    <h1 class="display-4">Verificar medidas</h1>

</div>


    

    <div class="form-group text-center">
            <div>@Html.DisplayFor(piso => piso.Local)</div>
            <div>Medida da área ( @Html.DisplayFor(piso => piso.Area1) mts x @Html.DisplayFor(piso => piso.Area2) mts ) =  @Html.DisplayFor(piso => piso.CalculoArea) metros²</div>
            <div>Medida do piso ( @Html.DisplayFor(piso => piso.Piso1) cm x @Html.DisplayFor(piso => piso.Piso2) cm ) =  @Html.DisplayFor(piso => piso.CalculoPiso) peças</div>
    </div><br />

        <div class="form-group text-center">
            <a class="btn btn-outline-primary" asp-controller="Piso" asp-action="Index" role="button">Voltar</a>
        </div>

   



  

  




   

    

